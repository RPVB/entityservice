{
    "name": "polderknowledge/entityservice",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.youngguns.nl"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "doctrine/collections": "1.2.*",
        "Youngguns/StdLib": ">=0.16,<1.0",
        "zendframework/zend-eventmanager": "2.3.*",
        "zendframework/zend-servicemanager": "2.3.*"
    },
    "require-dev": {
        "doctrine/orm": "2.4.*",
        "phpunit/phpunit": "4.4.*",
        "squizlabs/php_codesniffer": "2.*",
        "phing/phing": "2.10.*",
        "phpmd/phpmd": "1.*",
        "pdepend/pdepend": "1.*",
        "phpdocumentor/phpdocumentor": "~2.8",
        "zendframework/zend-paginator": "2.3.*",
        "zendframework/zend-validator": "2.3.*"
    },
    "autoload": {
        "psr-4": {
            "PolderKnowledge\\EntityService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PolderKnowledge\\EntityServiceTest\\": "tests/"
        }
    },
    "suggest": {
        "zendframework/zend-validator": "To make use of the validators provided by this package."
    }
}
